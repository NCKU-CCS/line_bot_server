{
    /* All the state must be definied here*/
    "states": [
        "user",
        "receive_user_language",
        {
            "name": "greet",
            "on_enter": {
                "name": "on_enter_greet",
                "oper_type": "custom",
                "custom_type": "text-finish",
                "template": "greeting.j2"

            }
        },
        {
            "name": "ask_usage",
            "on_enter": {
                "name": "on_enter_ask_usage",
                "oper_type": "text",
                "template": "manual.j2"

            }
        },
        "ask_dengue_fever",
        "ask_prevention",
        "ask_epidemic",
        "ask_symptom",
        {
            "name": "ask_breeding_source",
            "on_enter": {
                "name": "on_enter_ask_breeding_source",
                "oper_type": "custom",
                "custom_type": "text-finish",
                "template": "breeding_source.j2"
            }
        },
        {
            "name": "ask_who_we_are",
            "on_enter": {
                "name": "on_enter_ask_who_we_are",
                "oper_type": "custom",
                "custom_type": "text-finish",
                "template": "who_we_are.j2"
            }
        },
        {
            "name": "ask_self_prevention",
            "on_enter": {
                "name": "on_enter_ask_self_prevention",
                "oper_type": "custom",
                "custom_type": "text-finish",
                "template": "self_prevent.j2"
            }
        },
        {
            "name": "ask_env_prevention",
            "on_enter": {
                "name": "on_enter_ask_env_prevention",
                "oper_type": "custom",
                "custom_type": "text-finish",
                "template": "env_prevent.j2"
            }
        },
        "ask_hospital",
        "wait_user_location",
        "receive_user_location",
        "receive_user_address",
        {
            "name": "wait_user_suggestion",
            "on_enter": {
                "name": "on_enter_wait_user_suggestion",
                "oper_type": "text",
                "template": "ask_advice.j2"
            }
        },
        "ask_hospital_map",
        "gov_faculty_report",
        "wait_gov_location",
        "receive_gov_location",
        "user_join",
        "unrecognized_msg",
        "user_register_location",
        "wait_register_location",
        "receive_register_location"
    ],
    "transitions": [
        {
            "trigger": "advance",
            "source": "user",
            "dest": "user_join",
            "conditions": "is_follow_event"
        },
        {
            "trigger": "advance",
            "source": "user_join",
            "dest": "receive_user_language",
            "conditions": "is_valid_language"
        },
        {
            "trigger": "advance",
            "source": "user_join",
            "dest": "user_join",
            "conditions": "is_invalid_language"
        },
        {
            "trigger": "finish",
            "source": "receive_user_language",
            "dest": "user"
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_prevention",
            "conditions": [
                "is_text_message",
                "is_asking_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_prevention",
            "dest": "ask_self_prevention",
            "conditions": [
                "is_asking_self_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_prevention",
            "dest": "ask_env_prevention",
            "conditions": [
                "is_asking_env_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_self_prevention",
            "conditions": [
                "is_asking_self_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_env_prevention",
            "conditions": [
                "is_asking_env_prevention"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_dengue_fever",
            "conditions": [
                "is_text_message",
                "is_asking_dengue_fever"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_hospital",
            "conditions": [
                "is_text_message",
                "is_asking_hospital"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_hospital",
            "dest": "wait_user_location"
        },
        {
            "trigger": "advance",
            "source": "wait_user_location",
            "dest": "receive_user_location",
            "conditions": "is_location_message"
        },
        {
            "trigger": "advance",
            "source": "wait_user_location",
            "dest": "receive_user_address",
            "conditions": "is_text_message"
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_symptom",
            "conditions": [
                "is_text_message",
                "is_asking_symptom"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_epidemic",
            "conditions": [
                "is_text_message",
                "is_asking_epidemic"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "greet",
            "conditions": [
                "is_text_message",
                "is_greeting"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_breeding_source",
            "conditions": [
                "is_text_message",
                "is_asking_breeding_source"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_who_we_are",
            "conditions": [
                "is_text_message",
                "is_asking_who_we_are"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "wait_user_suggestion",
            "conditions": [
                "is_text_message",
                "is_giving_suggestion"
            ]
        },
        {
            "trigger": "advance",
            "source": "wait_user_suggestion",
            "dest": "user",
            "conditions": "is_pass"
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_usage",
            "conditions": [
                "is_text_message",
                "is_asking_usage"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "ask_dengue_fever",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_dengue_fever"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "ask_prevention",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "ask_hospital",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_hospital"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "ask_symptom",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_symptom"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "ask_epidemic",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_epidemic"
            ]
        },
        {
            "trigger": "advance",
            "source": "ask_usage",
            "dest": "wait_user_suggestion",
            "conditions": [
                "is_text_message",
                "is_selecting_give_suggestion"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_dengue_fever",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_dengue_fever"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_prevention",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_prevention"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_hospital",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_hospital"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_symptom",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_symptom"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_epidemic",
            "conditions": [
                "is_text_message",
                "is_selecting_ask_epidemic"
            ]
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "wait_user_suggestion",
            "conditions": [
                "is_text_message",
                "is_selecting_give_suggestion"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "ask_hospital_map",
            "conditions": [
                "is_postback_event",
                "is_hospital_address"
            ]
        },

        {
            "trigger": "advance",
            "source": "user",
            "dest": "gov_faculty_report",
            "conditions": [
                "is_text_message",
                "is_gov_report"
            ]
        },
        {
            "trigger": "advance",
            "source": "gov_faculty_report",
            "dest": "wait_gov_location"
        },
        {
            "trigger": "advance",
            "source": "wait_gov_location",
            "dest": "receive_gov_location",
            "conditions": "is_location_message"
        },
        {
            "trigger": "advance",
            "source": "user",
            "dest": "user_register_location",
            "conditions": [
                "is_text_message",
                "is_selecting_register_location"
            ]
        },
        {
            "trigger": "advance",
            "source": "user_register_location",
            "dest": "wait_register_location"
        },
        {
            "trigger": "advance",
            "source": "wait_register_location",
            "dest": "receive_register_location",
            "conditions": "is_location_message"
        },


        {
            "trigger": "finish_ans",
            "source": [
                "ask_hospital_map",
                "ask_self_prevention",
                "ask_env_prevention",
                "ask_dengue_fever",
                "receive_user_location",
                "receive_user_address",
                "ask_symptom",
                "ask_epidemic",
                "greet",
                "ask_breeding_source",
                "ask_who_we_are",
                "receive_gov_location",
                "receive_register_location"
            ],
            "dest": "user"
        },

        {
            "trigger": "advance",
            "source": [
                "user",
                "ask_usage",
                "ask_prevention",
                "wait_user_location",
                "wait_gov_location",
                "wait_register_location"
            ],
            "dest": "unrecognized_msg",
            "conditions": "is_pass"
        },
        {
            "trigger": "handle_unrecognized_msg",
            "source": "unrecognized_msg",
            "dest": "user"
        }
    ]
}
